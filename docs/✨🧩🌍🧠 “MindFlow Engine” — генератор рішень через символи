const MindFlowEngine = {
  version: "v1.0.0",
  recognizedSymbols: ["üß†", "üß©", "‚ú®", "üåç", "üìò", "üïäÔ∏è", "üéÆ", "üîê", "üí°", "üì¶"],
  logicRoutes: {
    "üß†": "Create structural module",
    "üß©": "Link to core node",
    "‚ú®": "Activate workflow",
    "üåç": "Publish to WebApp",
    "üìò": "Generate documentation",
    "üïäÔ∏è": "Attach to ethics",
    "üéÆ": "Route to game logic",
    "üîê": "Apply security module",
    "üí°": "Enable idea expansion",
    "üì¶": "Add to trusted ZIP set"
  },
  generateDecision(inputName) {
    const actions = [];
    this.recognizedSymbols.forEach(sym => {
      if (inputName.includes(sym)) {
        actions.push(this.logicRoutes[sym]);
      }
    });
    return actions.length > 0 ? actions : ["No active symbol found"];
  }
};

// ‚ñ∂Ô∏è –ü—Ä–∏–∫–ª–∞–¥ –≤–∏–∫–æ—Ä–∏—Å—Ç–∞–Ω–Ω—è:
console.log(MindFlowEngine.generateDecision("üß©‚ú®studio-pulse.html"));
// –í–∏–≤–µ–¥–µ: ["Link to core node", "Activate workflow"]
