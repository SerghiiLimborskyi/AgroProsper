const DAOCoreBox = {
  defaultOpen: true,
  accessLogic: {
    symbolCheck: fileName => /🧠|🧩|📘|✨|🛡️/.test(fileName),
    timeLimitDays: 10,
    verifier: (fileName, user) => {
      return {
        access: this.symbolCheck(fileName),
        reviewedUntil: Date.now() + this.timeLimitDays * 86400000,
        protectedBy: "Puzzle Interceptor 🛡️"
      };
    }
  },
  contents: [
    "🎮first-harvest-game.html",
    "🧠studio-runtime.html",
    "📘Studio Certificate V6.pdf",
    "🕊️humble-node.html",
